  spawn(function()
    if state then
      spawn(function()
        getgenv().watch = true;
        while watch do
          spawn(function()
            viewing = game.Players[_G.tplayer];
            workspace.CurrentCamera.CameraSubject = viewing.Character;
          end);
          wait();
        end
      end);
    else
      spawn(function()
        getgenv().watch = false;
        viewing = game.Players.LocalPlayer;
        workspace.CurrentCamera.CameraSubject = viewing.Character;
        wait();
        getgenv().watch = false;
        viewing = game.Players.LocalPlayer;
        workspace.CurrentCamera.CameraSubject = viewing.Character;
      end);
    end
  end);
